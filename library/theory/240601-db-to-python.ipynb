    "#Andrey Ivanchenko\n",
    "class Book:\n",
    "    def __init__(self, name, author, genre, count_pages):\n",
    "        self.name = name  \n",
    "        self.author = author\n",
    "        self.genre = genre\n",
    "        self.count_pages = count_pages\n",
    "        \n",
    "    def count_book_pages(self):\n",
    "        return self.count_pages\n",
    "       \n",
    "#тестируем подсчёт количества страниц в классе Book\n",
    "def test_count_book_pages():\n",
    "    book1 = Book('The old man and sea', 'Hemingway', 'Prose', 150)\n",
    "    assert book1.count_book_pages() == 150\n",
    "\n",
    "test_count_book_pages()\n",
    "\n",
    "class Library:\n",
    "    def __init__(self, books_list, user_carts)#, library_name, cursore ):\n",
    "        self.books_list = []\n",
    "        self.user_carts = {}\n",
    "        #elf.library_name = library_name\n",
    "        #self.__cursore = cursore \n",
    "        \n",
    "    def add_books(self, book):\n",
    "        self.books_list.append(book)\n",
    "    \n",
    "    def total_books(self):\n",
    "        return len(self.books_list)\n",
    "\n",
    "    def books_in_library(self):\n",
    "        return self.books_list\n",
    "        #return self.__cursore.execust(\"select * from library2book where library_name = self.library_name \")\n",
    "    \n",
    "    def avg_pages(self):\n",
    "        #sum_pages = sum(self.books_list)\n",
    "        sum_pages = 0\n",
    "        # смотрю книги по очереди, \n",
    "        # количество страниц в них \n",
    "        # и складываю друг с другом\n",
    "        for book in self.books_list:\n",
    "            sum_pages += book.count_book_pages()\n",
    "        len_books = len(self.books_list)\n",
    "        result = sum_pages / len_books\n",
    "        return result\n",
    "    \n",
    "    def search_book_name(self, search_book_name):\n",
    "        list_book_name = []\n",
    "        for book in self.books_list:\n",
    "            if search_book_name == book.name:\n",
    "               list_book_name.append(book)\n",
    "        return list_book_name\n",
    "        \n",
    "    def search_book_author(self, search_author):\n",
    "        list_book_author = []\n",
    "        for book in self.book:\n",
    "            if search_author == book.author:\n",
    "               list_book_author.append(book) \n",
    "        return list_book_author\n",
    "    \n",
    "    def search_book_genre(self, search_genre):\n",
    "        list_genre = []\n",
    "        for book in self.books_list:\n",
    "            if search_genre == book.genre:\n",
    "               list_genre.append(book) \n",
    "        return list_genre\n",
    "\n",
    "    def new_user(self, name_surname):\n",
    "        self.user_carts[name_surname] = []\n",
    "\n",
    "    def take_book(self, name_surname, the_book): #метод добавления книги в карту пользователя (не более 5шт)\n",
    "        if name_surname in self.user_carts:\n",
    "            if len(self.user_carts[name_surname]) < 5:\n",
    "                for book in self.books_list:\n",
    "                if the_book == book:\n",
    "                    self.user_carts[name_surname].append(the_book)\n",
    "                else:\n",
    "                    return \"No such a book in the library\"\n",
    "            else:\n",
    "                return \"You try to take too many books for one account\"\n",
    "        else:\n",
    "            return \"You don't have an account in the Library\"\n",
    "\n",
    "    def return_book(self, name_surname, the_book): #метод возврата книги в библиотеку\n",
    "        if name_surname in self.user_carts:\n",
    "            for book in self.user_carts[name_surname]:\n",
    "                if the_book == book:\n",
    "                    self.user_carts[name_surname].remove(the_book)\n",
    "                    self.books_list.append(the_book)\n",
    "                else:\n",
    "                    return 'Are you sure you took this book in our Library?'\n",
    "        else:\n",
    "            return 'Are you sure you have an account in our Library?'\n",
    "\n",
    "def test_total_books():\n",
