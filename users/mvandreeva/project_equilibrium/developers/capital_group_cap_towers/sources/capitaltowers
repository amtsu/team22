<script>
    	$( function() {
            var sliders = $( ".ui-ranger" );
            sliders.each(function () {
                $(this).slider({
                    range:true,
                    min: $(this).data('min'),
                    max: $(this).data('max'),
                    values: $(this).data('values'),
                    slide: function (event, ui) {
                        $(ui.handle).closest('.calculator__input').find('[data-input-min]').val(ui.values[0]);
                        $(ui.handle).closest('.calculator__input').find('[data-input-max]').val(ui.values[1]);
                    }
                });
            });
        } );
        
        function change_slider(e)
        {
            var sliders = $(e).closest('.calculator__input').find('.ui-ranger');
            var min = Number(sliders.data('min'));
            var max = Number(sliders.data('max'));
            
            var min_input =$(e).closest('.calculator__result').find('[data-input-min]');
            var max_input =$(e).closest('.calculator__result').find('[data-input-max]');
            
            var min_val = Number(min_input.val());
            var max_val = Number(max_input.val());
            
           
            if(min_val<min){
                min_val = min;
            }
           
            if(max_val>max){
                max_val = max;
            }
            
            if(min_val>max_val) {
                max_val = max;
                min_val = min;
            }

            min_input.val(min_val);
            max_input.val(max_val);
            $(e).closest('.calculator__input').find('.ui-ranger').slider( "option", "values",[min_val,max_val] );
        }
        
        $("[data-input-min]").on("change", function () {
           change_slider(this);
        });
         $("[data-input-max]").on("change", function () {
           change_slider(this);
        });
    </script>
