  104  touch file_1
  105  mkdir work_dir
  106  cp file_1 work_dir/file_1_copy
  107  cp -r work_dir work_dir_new
  108  mv file_1_copy file_1_new_name
  109  cd work_dir
  110  mv file_1_copy file_1_new_name
