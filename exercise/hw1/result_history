    1  ssh-keygen -t rsa -b 4096 -C anna.arisova@gmail.com
    2  cat ~/.ssh/id_rsa.pub
    3  mkdir github
    4  cd github
    5  git clone git@github.com:amtsu/team22.git
    6  git clone https://github.com/amtsu/team22.git
    7  cd github/team22
    8  git branch aaarisova
    9  git checkout aaarisova
   10  cd users
   11  mkdir arisova
   12  cd arisova/
   13  touch README.md
   14  pwd > README.md
   15  cd ../..
   16  git status
   17  git diff
   18  git add users/arisova/README.md
   19  vim arisova/
   20  sudo vim arisova
   21  vim users/arisova/README.md
   22  cat users/arisova/README.md
   23  cat README.md
   24  cd github/team22/users/aaarisova
   25  cd github/team22/users/arisova
   26  cat README.md
   27  pwd > README.md
   28  cat README.md
   29  vim README.md
   30  cd ../..
   31  git status
   32  git diff
   33  git add users/arisova/README.md
   34  git config --global user.email anna.arisova@gmail.com
   35  git config --global user.name aaarisova
   36  git config --global user.name arisova
   37  git commit -m 'Created README.md with a description about myself'
   38  git push --set-upstream origin aaarisova
   39  git status
   40  git push --set-upstream origin aaarisova
   41  git remote -v
   42  cd ~/github/team22/
   43  ls
   44  cd users
   45  ls
   46  mv arisova aaarisova
   47  ls
   48  git checkout aaarisova
   49  git branch
   50  git status
   51  cp arisova/README.md aaarisova/README.md
   52  cd aaarisova
   53  vim README.md
   54  git status
   55  git add README.md
   56  git status
   57  git commit -m "Added information about myself to README.md"
   58  git push
   59  git push --set-upstream origin aaarisova
   60  vim users/arisova/README.md
   61  git branch
   62  cd ~/github/team22
   63  git branch
   64  cat README.md
   65  cd ~/github/team22
   66  cat README.md
   67  cat users/aaarisova/README.md
   68  cd ~/github/team22
   69  git push --set-upstream origin aaarisova
   70  git remote set-url origin git@github.com:amtsu/team22.git
   71  git push --set-upstream origin aaarisova
   72  git checkout main
   73  cd ~/github/team22/
   74  git checkout main
   75  git pull origin main
   76  git checkout -b aarisova-hw2-part2
   77  cd ~/github/team22
   78  mkdir exercise
   79  cd exercise
   80  mkdir hw1
   81  cd hw1
   82  git branch
   83  pwd
   84  ls -la
   85  ls /
   86  cd /home
   87  ls -la
   88  git status
   89  cd ~/github/team22/exercise/hw1
   90  git add .
   91  git commit -m "Task_1"
   92  git diff
   93  git status
   94  du .
   95  du . > hw1/result_du
   96  du . > result_du
   97  history
   98  history > result_history
